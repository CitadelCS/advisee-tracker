<p id="alert"><%= alert %></p>
<h1>Add New Student</h1>
<%= form_tag students_path do |form| %>
  <div class="field">
    <%= label_tag :First_Name %>
    <%= text_field_tag :text %>
  </div>
  <div class="field">
    <%= label_tag :Last_Name %>
    <%= text_field_tag :text %>
  </div>
<div class="field">
    <%= label_tag :CWID %>
    <%= text_field_tag :text %>
  </div>
<div class="field">
    <%= label_tag :DOB %>
    <%= date_field_tag :date %> 
</div>
<div class="field">
    <%= label_tag :Advisor %>
    <%= text_field_tag :text %>
  </div>
<div class="field">
    <%= label_tag :Graduation_Year%>
    <%= text_field_tag :text %>
  </div>
<div class="field">
    <%= label_tag :Graduation_Semester%>
    <%= text_field_tag :text %>
  </div>

  <div class="actions">
    <%= submit_tag "add" %>
  </div>
<% end %>
